<div class="header">
  <h1 class="title_link"><%= link_to 'Justin Floors Inc', root_path %></h1>
  <h2>Showcase</h2>
</div>

<div class="body">


  <%= @photos.each do |photo| %>
  <%= link_to image_tag(photo.file.thumb('200x200#').url),
       photo.file.remote_url,
    class: 'fancybox', rel: 'group', title: photo.title, alt: 'Photo' %>'
   <% end %>


</div>
